@model IEnumerable<NetCoreLAB6_EF.Models.Category>

<h2>Danh sách Categories</h2>
<a asp-action="Create">Thêm mới</a>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Tên Category</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>
